###
ssrp
--crypt none --mode fast --sndwnd 512 --rcvwnd 1024 --dscp 1 --nocomp true
OneX
key=onex;crypt=none;mode=fast;sndwnd=512;rcvwnd=1024;dscp=1;nocomp=true
###
sed -i 's/#src-git/src-git/g' feeds.conf.default
###
CPU频率
cat /sys/devices/system/cpu/cpu[00]/cpufreq/cpuinfo_cur_freq
cat /sys/devices/system/cpu/cpu[01]/cpufreq/cpuinfo_cur_freq
cat /sys/devices/system/cpu/cpu[02]/cpufreq/cpuinfo_cur_freq
cat /sys/devices/system/cpu/cpu[03]/cpufreq/cpuinfo_cur_freq
cat /sys/devices/system/cpu/cpu[04]/cpufreq/cpuinfo_cur_freq
cat /sys/devices/system/cpu/cpu[05]/cpufreq/cpuinfo_cur_freq

cat /sys/devices/system/cpu/cpufreq/policy0/cpuinfo_cur_freq
cat /sys/devices/system/cpu/cpufreq/policy4/cpuinfo_cur_freq
CPU温度
cat /sys/class/thermal/thermal_zone0/temp
cat /sys/class/thermal/thermal_zone1/temp

压测
dd if=/dev/zero of=/dev/null

dd if=/dev/zero of=/dev/null bs=1&
dd if=/dev/zero of=/dev/null bs=1&
dd if=/dev/zero of=/dev/null bs=1&
dd if=/dev/zero of=/dev/null bs=1&
dd if=/dev/zero of=/dev/null bs=1&
dd if=/dev/zero of=/dev/null bs=1&

netstat -tunlp
